<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="rootPane" fx:controller="sample.ModifyPartViewController" prefHeight="500.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label layoutX="15.0" layoutY="23.0" prefHeight="59.0" prefWidth="159.0" text="modify Part">
            <font>
                <Font size="27.0" />
            </font>
        </Label>
        <RadioButton fx:id="inHouse" onAction="#inHouseDial" layoutX="150.0" layoutY="95.0" mnemonicParsing="false" text="In-House">
            <font>
                <Font size="15.0" />
            </font>
        </RadioButton>
        <RadioButton fx:id="outSourced" onAction="#outSourceDial" layoutX="346.0" layoutY="96.0" mnemonicParsing="false" text="Outsourced">
            <font>
                <Font size="15.0" />
            </font>
        </RadioButton>
        <!--Need to modify id field lable-->
        <Label layoutX="137.0" layoutY="140.0" prefHeight="25.0" prefWidth="45.0" text="Name">
            <font>
                <Font size="15.0" />
            </font>
        </Label>
        <Label layoutX="137.0" layoutY="195.0" prefHeight="26.0" prefWidth="27.0" text="Inv">
            <font>
                <Font size="15.0" />
            </font>
        </Label>
        <Label layoutX="137.0" layoutY="250.0" prefHeight="26.0" prefWidth="70.0" text="Price/Cost">
            <font>
                <Font size="15.0" />
            </font>
        </Label>
        <Label layoutX="138.0" layoutY="304.0" prefHeight="33.0" prefWidth="35.0" text="Max">
            <font>
                <Font size="15.0" />
            </font>
        </Label>
        <Label layoutX="305.0" layoutY="304.0" prefHeight="32.0" prefWidth="30.0" text="Min">
            <font>
                <Font size="15.0" />
            </font>
        </Label>
        <!--Need to modify ID field-->
        <TextField fx:id="modifyPartName" layoutX="304.0" layoutY="140.0" prefHeight="25.0" prefWidth="176.0" />
        <TextField fx:id="modifyPartInv" layoutX="304.0" layoutY="195.0" prefHeight="25.0" prefWidth="176.0" />
        <TextField fx:id="modifyPartPrice" layoutX="304.0" layoutY="250.0" prefHeight="25.0" prefWidth="176.0" />
        <TextField fx:id="modifyPartMin" layoutX="176.0" layoutY="303.0" prefHeight="25.0" prefWidth="111.0" />
        <TextField fx:id="modifyPartMax" layoutX="340.0" layoutY="303.0" prefHeight="25.0" prefWidth="111.0" />


        <Button onAction="#modifyPartSave" layoutX="339.0" layoutY="360.0" mnemonicParsing="false" prefHeight="51.0" prefWidth="90.0" text="Save" />
        <Button onAction="#modifyPartCancel" layoutX="491.0" layoutY="360.0" mnemonicParsing="false" prefHeight="51.0" prefWidth="108.0" text="Cancel" />
    </children>
</AnchorPane>
